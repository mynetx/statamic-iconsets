    "keywords": [
        "statamic",
        "icons",
        "fieldtype",
        "svg",
        "icon-picker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim RÃ¼tter"
        }
    ],
    "support": {
        "email": "me@mynetx.net"
    },
    "require": {
        "php": "^8.1",
        "statamic/cms": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Mynetx\\Iconsets\\": "src/"
        }
    },
    "extra": {
